{
    "name": "textlint-plugin-org",
    "version": "1.0.0",
    "description": "Org mode parser for textlint",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/robstewart57/textlint-plugin-org.git"
    },
    "author": "Rob Stewart",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/robstewart57/textlint-plugin-org/issues"
    },
